{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/al-buraq/browser"
      }
    },
    {
      "src": "dist/al-buraq/server/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/al-buraq/server/main.js"
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}
